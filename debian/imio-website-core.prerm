#!/bin/sh
set -e
set -x

PACKAGE="imio-website-core"


case "$1" in
    remove|deconfigure)
	find /home/imio/imio-website/eggs /home/imio/imio-website/devel -name "*.pyc" -delete;
	find /home/imio/imio-website/eggs /home/imio/imio-website/devel -name "*.pyo" -delete;
	find /home/imio/imio-website/eggs /home/imio/imio-website/devel -name "*.bin" -delete;
    ;;

    upgrade|failed-upgrade)
    ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 0
    ;;
esac

exit 0
